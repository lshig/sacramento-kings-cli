{
  "name": "sacramento-kings-cli",
  "version": "0.0.0",
  "description": "CLI for Sacramento Kings updates",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "coverage": "jest --coverage",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write '**/*.(ts|md)' *.js",
    "lint": "npm run format && npm run lint-es && npm run lint-md",
    "lint-es": "eslint '**/*.ts' '**/*.js'",
    "lint-md": "remark .",
    "test": "npm run lint && jest --coverage",
    "test:watch": "jest --verbose --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lshig/sacramento-kings-cli.git"
  },
  "author": "lshig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lshig/sacramento-kings-cli/issues"
  },
  "homepage": "https://github.com/lshig/sacramento-kings-cli#readme",
  "bin": {
    "sac": "dist/index.js"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "axios": "^0.24.0",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "eslint": "^8.1.0",
    "lodash": "^4.17.21",
    "node-emoji": "^1.11.0",
    "ora": "^5.4.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/node-emoji": "^1.8.1",
    "@types/yargs": "^17.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "remark-cli": "^10.0.0",
    "remark-preset-lint-recommended": "^6.1.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
