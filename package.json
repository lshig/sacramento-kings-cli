{
  "name": "sacramento-kings-cli",
  "version": "0.0.0",
  "description": "CLI for Sacramento Kings updates",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write '**/*.(ts|md)' *.js",
    "lint": "npm run format && npm run lint-es && npm run lint-md",
    "lint-es": "eslint '**/*.ts' '**/*.js'",
    "lint-md": "remark ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lshig/sacramento-kings-cli.git"
  },
  "author": "lshig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lshig/sacramento-kings-cli/issues"
  },
  "homepage": "https://github.com/lshig/sacramento-kings-cli#readme",
  "bin": {
    "sac": "dist/index.js"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "axios": "^0.21.1",
    "boxen": "^5.0.0",
    "chalk": "^4.1.0",
    "eslint": "^7.20.0",
    "lodash": "^4.17.20",
    "node-emoji": "^1.10.0",
    "ora": "^5.3.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.28",
    "@types/node-emoji": "^1.8.1",
    "@types/yargs": "^16.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.2.1",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
