{
  "name": "sacramento-kings-cli",
  "version": "0.0.0",
  "description": "CLI for Sacramento Kings updates",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "coverage": "jest --coverage",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write '**/*.(ts|md)' *.js",
    "lint": "npm run format && npm run lint-es && npm run lint-md",
    "lint-es": "eslint '**/*.ts' '**/*.js'",
    "lint-md": "remark .",
    "test": "npm run lint && jest --coverage",
    "test:watch": "jest --verbose --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lshig/sacramento-kings-cli.git"
  },
  "author": "lshig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lshig/sacramento-kings-cli/issues"
  },
  "homepage": "https://github.com/lshig/sacramento-kings-cli#readme",
  "bin": {
    "sac": "dist/index.js"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "eslint": "^8.53.0",
    "lodash": "^4.17.21",
    "node-emoji": "^2.1.0",
    "ora": "^5.4.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/node-emoji": "^2.1.0",
    "@types/yargs": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "remark-cli": "^12.0.0",
    "remark-preset-lint-recommended": "^6.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
